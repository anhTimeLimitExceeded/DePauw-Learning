    "vue": "^2.6.10",
    "vue-monaco": "^1.1.0",
    "vue-router": "^3.0.3",
    "vue-sidebar-menu": "^4.2.3",
    "vuex": "^3.0.1",
    "webstomp-client": "^1.2.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.2",
    "@fortawesome/fontawesome-free": "^5.11.2",
